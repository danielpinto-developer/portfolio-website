{
    "version": 2,
    "builds": [
      {
        "src": "api/submit-form.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/submit-form",
        "dest": "/api/submit-form.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  